{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["deno.ns", "dom", "esnext"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "types": [],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "../../node_modules"]
}